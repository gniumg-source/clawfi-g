{
  "name": "@clawfi/core",
  "version": "0.1.0",
  "private": true,
  "description": "Core domain models, schemas, and types for ClawFi",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./signals": {
      "types": "./dist/signals/index.d.ts",
      "import": "./dist/signals/index.js"
    },
    "./risk": {
      "types": "./dist/risk/index.d.ts",
      "import": "./dist/risk/index.js"
    },
    "./strategies": {
      "types": "./dist/strategies/index.d.ts",
      "import": "./dist/strategies/index.js"
    },
    "./audit": {
      "types": "./dist/audit/index.d.ts",
      "import": "./dist/audit/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}


