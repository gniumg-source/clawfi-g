{
  "name": "clawfi",
  "version": "0.1.0",
  "private": true,
  "description": "ClawFi - All-Access Crypto Agent with CEX/DEX connectors, strategy engine, and risk management",
  "author": "ClawFi Team",
  "license": "MIT",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "pnpm --filter @clawfi/node db:generate",
    "db:migrate": "pnpm --filter @clawfi/node db:migrate",
    "db:push": "pnpm --filter @clawfi/node db:push",
    "db:seed": "pnpm --filter @clawfi/node db:seed",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "packageManager": "pnpm@8.14.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}

